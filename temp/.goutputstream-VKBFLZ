import java.util.*;
import java.io.*;


public class test {
    static find(char[] s, char x) {
        int pos = 0;
        for(char c : s) {
            if(c == x)
                return pos;
            ++pos;
        }
        return -1;
    }
    
    public static void main(String[] args) throws IOException {
        Scanner in = new Scanner(System.in);
        
        int n = in.nextInt();
        int k = in.nextInt();
        
        in.nextLine();
        String str = new String(in.nextLine());
        char[] s = str.toCharArray();
        
        for(int i = 0; i < k; ++i) {
            // Something;
        }
    }
}
